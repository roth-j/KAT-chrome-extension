{
  "name": "kill-active-tab",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "KAT",
    "default_icon": "icon.png"
  },
  "permissions": [
    "tabs"
  ]
}
